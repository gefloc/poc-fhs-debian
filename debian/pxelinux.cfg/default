menu hidden
prompt 1
timeout 1

default deblive

label deblive
    menu label Debian Live
    kernel live/vmlinuz
    append initrd=live/initrd.img ip=dhcp netboot=nfs root=/dev/nfs nfsroot=192.168.178.10:/var/netboot nfsrootdebug boot=live components hostname=FHS-Debian username=lehrer user-fullname=Lehrer locales="de_DE.UTF-8" keyboard-layouts=de hooks=filesystem noroot

ui menu.c32
